import java.util.*;
class Main {
    public static int f(int i,int n){
        if(n<10){
            return i+n;
        }
        int m=(i+(n%10))*10;
        return f(m,n/10);
        
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println(f(0,sc.nextInt()));
    }
}
