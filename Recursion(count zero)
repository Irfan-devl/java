import java.util.*;
class Main {
    public static int f(int i,int n){
        if(n==0){
            return i;
        }
        int m=n%10;
        if(m==0){
            ++i;
        }
        return f(i,n/10);
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println(f(0,sc.nextInt()));
    }
}
