import java.util.*;
class Main {
    public static int f(int n){
        if(n<1){
            return n;
        }
        return (n%10)+f(n/10);
        
    }
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println(f(sc.nextInt()));
    }
}
